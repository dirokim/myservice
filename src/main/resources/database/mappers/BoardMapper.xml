<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.summer.app.board.BoardDAO">

	<insert id="createBoard">
	INSERT INTO BOARD
	(
		MEMBER_ID
		,BOARD_WRITER
		,BOARD_HEAD
		,BOARD_CONTENTS
		,BOARD_DATE
	)
	VALUES 
	(		
	   	#{member_id}
		,#{board_writer}
		,#{board_head}
		,#{board_contents}
		,#{board_date}
	)
	</insert>

</mapper>